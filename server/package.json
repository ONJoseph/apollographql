    },
    "devDependencies": {
      "@graphql-codegen/cli": "^4.0.1",
      "@graphql-codegen/typescript": "^4.0.1",
      "@graphql-codegen/typescript-resolvers": "^4.0.1",
      "dotenv": "^8.2.0",
      "nodemon": "^2.0.4",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.1.3"
    },
    "author": "Raphael Terrier @R4ph-t",
